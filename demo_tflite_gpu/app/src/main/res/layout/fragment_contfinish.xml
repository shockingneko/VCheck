<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/finishbackground"
    tools:context=".contrast.contfinishFragment">

    <Button
        android:id="@+id/conbt"
        android:layout_width="140dp"
        android:layout_height="60dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/buttonbackground"
        android:text="返回首頁"
        android:textColor="#504C4C"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/zzz123"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintStart_toStartOf="@+id/zzz123"
        app:layout_constraintTop_toBottomOf="@+id/zzz123"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/contrast1"
        android:layout_width="187dp"
        android:layout_height="36dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="對比度測試結果"
        android:textColor="#408C7E"
        android:textSize="25sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/contrast2"
        android:layout_width="142dp"
        android:layout_height="79dp"
        android:layout_marginTop="40dp"
        android:gravity="center_horizontal"
        android:text="正常"
        android:textColor="#408C7E"
        android:textSize="50sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrast1"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:gravity="center"
        android:text="測驗結束，以下是測驗結果"
        android:textColor="#FFFFFF"
        android:textSize="23sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/zzz123"
        android:layout_width="222dp"
        android:layout_height="27dp"
        android:layout_marginTop="36dp"
        android:text="建議您尋求專業醫師的幫助"
        android:textColor="#F69336"
        android:textSize="18sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrast2" />
</android.support.constraint.ConstraintLayout>